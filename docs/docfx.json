{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/xrefmap.yml",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/style-guide/breadcrumb/toc.json",
      "searchScope": [
        "Style guide"
      ],
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/microsoft-style-guide",
      "extendBreadcrumb": true
    },
    "fileMetadata": {},
    "template": [],
    "dest": "styleguide",
    "markdownEngineName": "markdig"
  }
}
